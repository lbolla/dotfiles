apiVersion: yougov.com/v1alpha2
kind: YGPassSecret
metadata:
  name: k8s-stingray
  namespace: {{ .Release.Namespace }}
spec:
  data:
  - name: STINGRAY_PASSWORD
    resource: {{ .Values.stingray.pmpAccount.resource }}
    username: {{ .Values.stingray.pmpAccount.user }}